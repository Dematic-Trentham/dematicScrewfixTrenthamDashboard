generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model errors{
    ID           Int       @id @default(autoincrement())
    error        String    @unique(map: "error") @default("0") @db.VarChar(75)
    lastIssue DateTime? @default(now()) @db.Timestamp(0)
    
    @@index([error], map: "key")
}