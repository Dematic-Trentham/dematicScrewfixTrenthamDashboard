{
  "name": "dematic_Screwfix_Trentham_Dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
    "make-permissions": "node ./tools/makePermissions.js",
    "update-all": "npm run prisma-merge && npm run prisma-push && npm run make-permissions",
    "prisma-merge": "prisma format --schema=./node_modules/masterPrismaSchema/prisma/schema/ && npm run prisma-gen && echo 'complete!'",
    "prisma-gen": "prisma generate --schema=./node_modules/masterPrismaSchema/prisma/schema",
    "prisma-pull": "npx prisma db pull --schema=./node_modules/masterPrismaSchema/prisma/schema",
    "prisma-push": "npx prisma db push --schema=./node_modules/masterPrismaSchema/prisma/schema",
    "packageJsonStripper": "node tools/packageJsonStripper.js",
    "build-docker": "npm run prisma-merge && npm run packageJsonStripper && docker build -t screwfix-trentham-dashboard ."
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.16.7",
    "@nextui-org/button": "^2.0.37",
    "@nextui-org/input": "^2.2.4",
    "@nextui-org/react": "^2.4.6",
    "@nextui-org/switch": "^2.0.33",
    "@nextui-org/system": "^2.2.5",
    "@nextui-org/table": "^2.0.39",
    "@nextui-org/theme": "^2.2.9",
    "@prisma/client": "^5.19.1",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cookies-next": "^4.2.1",
    "framer-motion": "^11.5.4",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "next-client-cookies": "^1.1.1",
    "next-cookies": "^2.0.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-tabs": "^6.0.2",
    "react-toastify": "^10.0.5",
    "zod": "^3.23.8"
  }
}