	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"make-permissions": "node ./tools/makePermissions.js",
		"update-all": "npm run prisma-merge && npm run prisma-push && npm run make-permissions",
		"prisma-merge": "prisma format --schema=./node_modules/masterPrismaSchema/prisma/schema/ && npm run prisma-gen && echo 'complete!'",
		"prisma-gen": "prisma generate --schema=./node_modules/masterPrismaSchema/prisma/schema",
		"prisma-pull": "npx prisma db pull --schema=./node_modules/masterPrismaSchema/prisma/schema",
		"prisma-push": "npx prisma db push --schema=./node_modules/masterPrismaSchema/prisma/schema",
		"packageJsonStripper": "node tools/packageJsonStripper.js",
		"build-docker": "npm run prisma-merge && npm run packageJsonStripper && docker build -t dematicscrewfixtrenthamdashboard .",
		"test": "jest",
		"npm-check": "npm-check",
		"lint:fix": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
		"check-updates": "ncu",
		"check-updates:upgrade": "ncu -u && npm install"
	},
